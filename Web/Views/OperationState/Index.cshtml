@model IEnumerable<Web.Models.Domain.OperationState>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RemittanceState)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MerchandiseState)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Remittance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Entry)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dock)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RemittanceState)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MerchandiseState)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Remittance.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Entry.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dock.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
